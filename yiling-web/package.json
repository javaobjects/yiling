{
  "name": "yiling-web-pop",
  "version": "0.1.0",
  "scripts": {
    "dev-pop": "cross-env PRO_NAME=pop vue-cli-service serve",
    "dev-admin": "cross-env PRO_NAME=admin vue-cli-service serve",
    "test-pop": "cross-env PRO_NAME=pop vue-cli-service serve --mode testing",
    "test-admin": "cross-env PRO_NAME=admin vue-cli-service serve --mode testing",
    "build:prod:admin": "cross-env PRO_NAME=admin vue-cli-service build",
    "build:dev:admin": "cross-env PRO_NAME=admin vue-cli-service build --mode development",
    "build:test:admin": "cross-env PRO_NAME=admin vue-cli-service build --mode testing",
    "build:prod:pop": "cross-env PRO_NAME=pop vue-cli-service build",
    "build:dev:pop": "cross-env PRO_NAME=pop vue-cli-service build --mode development",
    "build:test:pop": "cross-env PRO_NAME=pop vue-cli-service build --mode testing",
    "preview": "node build/index.js --preview",
    "report-admin": "cross-env useAnalyzer=true npm run build:prod:admin",
    "report-pop": "cross-env useAnalyzer=true npm run build:prod:pop",
    "test": "node serve.js",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "axios": "0.18.1",
    "core-js": "3.6.5",
    "echarts": "5.1.2",
    "element-ui": "2.15.3",
    "js-cookie": "2.2.0",
    "lodash": "^4.17.21",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "vue": "2.6.11",
    "vue-dompurify-html": "^2.5.0",
    "vue-router": "3.0.2",
    "vuex": "3.6.2",
    "wangeditor": "^4.7.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.4.4",
    "@vue/cli-plugin-eslint": "4.4.4",
    "@vue/cli-plugin-unit-jest": "4.4.4",
    "@vue/cli-service": "4.4.4",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "23.6.0",
    "babel-plugin-component": "1.1.1",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chalk": "2.4.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "compression-webpack-plugin": "6.1.1",
    "connect": "3.6.6",
    "copy-webpack-plugin": "^5.1.2",
    "cross-env": "7.0.3",
    "cssnano": "4.1.11",
    "eslint": "6.7.2",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "3.2.0",
    "mockjs": "1.0.1-beta3",
    "node-sass": "6.0.0",
    "plop": "^2.7.4",
    "postcss-loader": "5.3.0",
    "postcss-px-to-viewport": "1.1.1",
    "postcss-viewport-units": "0.1.6",
    "runjs": "4.3.2",
    "sass": "1.26.8",
    "sass-loader": "8.0.2",
    "script-ext-html-webpack-plugin": "2.1.3",
    "serve-static": "1.13.2",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.2",
    "vue-template-compiler": "2.6.11",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">=14.9",
    "npm": ">= 3.0.0"
  },
  "license": "MIT"
}
